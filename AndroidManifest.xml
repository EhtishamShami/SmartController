<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.raofyp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />

    <uses-permission android:name="com.samsung.android.providers.context.permission.WRITE_USE_APP_FEATURE_SURVEY" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name=".Navigation"
            android:label="@string/title_activity_navigation" >
        </activity>
        <activity
            android:name=".Rooms"
            android:label="@string/title_activity_rooms" >
        </activity>
        <activity
            android:name=".Dining"
            android:label="@string/title_activity_dining" >
        </activity>
        <activity
            android:name=".Sitting"
            android:label="@string/title_activity_sitting" >
        </activity>
        <activity
            android:name=".BedRoom"
            android:label="@string/title_activity_bed_room" >
        </activity>
    </application>

</manifest>
